{
  "name": "pockemon-catalog",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --config webpack.dev.config.js  --progress --profile --watch",
    "build": "webpack --config webpack.dev.config.js",
    "cy:open": "cypress open",
    "cy:run": "start-server-and-test 'npm run start' http://127.0.0.1:4300 'cypress run'",
    "cy-component:run": "cypress run --component",
    "cy-e2e:open": "cypress open --config-file cypress.e2e.config.ts",
    "cy-e2e:run": "cypress run --config-file cypress.e2e.config.ts",
    "ci:copy-component-reports": "mkdir -p target/reports && mv .nyc_output/out.json target/reports/component.json && mv coverage target/component",
    "ci:copy-integration-reports": "mkdir -p target/reports && mv .nyc_output/out.json target/reports/integration.json && mv coverage target/integration",
    "ci:cy-run-component": "npm run cy-component:run && npm run ci:copy-component-reports",
    "ci:cy-run": "npm run cy:run",
    "ci:cy-run-e2e": "npm run cy-e2e:run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@lit-labs/context": "^0.1.3",
    "@lit/context": "^1.1.0",
    "lit": "3",
    "lit-css-loader": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@cypress/code-coverage": "^3.12.1",
    "@shellygo/cypress-test-utils": "^2.0.54",
    "babel-loader": "9.1.2",
    "builder-pattern": "^2.2.0",
    "chance": "^1.1.11",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.8.1",
    "cypress": "^13.3.1",
    "esbuild-loader": "^3.0.1",
    "html-webpack-plugin": "5.5.3",
    "lit-scss-loader": "^2.0.1",
    "mini-css-extract-plugin": "2.7.6",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "rollup-plugin-postcss-lit": "^2.0.0",
    "rollup-plugin-scss-lit": "^1.1.5",
    "sass": "1.63.3",
    "sass-loader": "13.3.2",
    "start-server-and-test": "^2.0.0",
    "style-loader": "3.3.3",
    "svelte": "^3.59.1",
    "terser-webpack-plugin": "5.3.9",
    "vite": "^4.3.9",
    "vite-plugin-istanbul": "^5.0.0",
    "webpack": "5.86.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.9.0",
    "webpack-plugin-istanbul": "^1.0.3"
  },
  "browser": {
    "[module-name]": false
  },
  "engines": {
    "npm": ">=9.8.1",
    "node": ">=18.18.2"
  },
  "nyc": {
    "all": true,
    "include": "src/**/*.ts"
  }
}
